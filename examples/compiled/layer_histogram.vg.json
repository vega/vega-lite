{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "autosize": "pad",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/flights-2k.json",
      "format": {"type": "json"}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "extent",
          "field": "distance",
          "signal": "layer_0_bin_maxbins_10_distance_extent"
        },
        {
          "type": "bin",
          "field": "distance",
          "as": ["bin_maxbins_10_distance", "bin_maxbins_10_distance_end"],
          "signal": "layer_0_bin_maxbins_10_distance_bins",
          "maxbins": 10,
          "extent": {"signal": "layer_0_bin_maxbins_10_distance_extent"}
        },
        {
          "type": "aggregate",
          "groupby": ["bin_maxbins_10_distance", "bin_maxbins_10_distance_end"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "extent",
          "field": "distance",
          "signal": "layer_1_bin_maxbins_10_distance_extent"
        },
        {
          "type": "bin",
          "field": "distance",
          "as": ["bin_maxbins_10_distance", "bin_maxbins_10_distance_end"],
          "signal": "layer_1_bin_maxbins_10_distance_bins",
          "maxbins": 10,
          "extent": {"signal": "layer_1_bin_maxbins_10_distance_extent"}
        },
        {"type": "filter", "expr": "datum.delay < 500"},
        {
          "type": "aggregate",
          "groupby": ["bin_maxbins_10_distance", "bin_maxbins_10_distance_end"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "rect",
      "style": ["bar"],
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "fill": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"]) || datum[\"__count\"] === null || isNaN(datum[\"__count\"])",
              "value": null
            },
            {"value": "#4c78a8"}
          ],
          "tooltip": {
            "signal": "{\"distance (binned)\": datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"]) ? \"null\" : format(datum[\"bin_maxbins_10_distance\"], \"\") + \" - \" + format(datum[\"bin_maxbins_10_distance_end\"], \"\"), \"Count of Records\": format(datum[\"__count\"], \"\")}"
          },
          "x2": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"])",
              "value": 0
            },
            {"scale": "x", "field": "bin_maxbins_10_distance", "offset": 1}
          ],
          "x": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"])",
              "value": 0
            },
            {"scale": "x", "field": "bin_maxbins_10_distance_end"}
          ],
          "y": {"scale": "y", "field": "__count"},
          "y2": {"scale": "y", "value": 0}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "rect",
      "style": ["bar"],
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "fill": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"]) || datum[\"__count\"] === null || isNaN(datum[\"__count\"])",
              "value": null
            },
            {"value": "goldenrod"}
          ],
          "tooltip": {
            "signal": "{\"distance (binned)\": datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"]) ? \"null\" : format(datum[\"bin_maxbins_10_distance\"], \"\") + \" - \" + format(datum[\"bin_maxbins_10_distance_end\"], \"\"), \"Count of Records\": format(datum[\"__count\"], \"\")}"
          },
          "x2": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"])",
              "value": 0
            },
            {"scale": "x", "field": "bin_maxbins_10_distance", "offset": 1}
          ],
          "x": [
            {
              "test": "datum[\"bin_maxbins_10_distance\"] === null || isNaN(datum[\"bin_maxbins_10_distance\"])",
              "value": 0
            },
            {"scale": "x", "field": "bin_maxbins_10_distance_end"}
          ],
          "y": {"scale": "y", "field": "__count"},
          "y2": {"scale": "y", "value": 0}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "fields": [
          {
            "signal": "[layer_0_bin_maxbins_10_distance_bins.start, layer_0_bin_maxbins_10_distance_bins.stop]"
          },
          {
            "signal": "[layer_1_bin_maxbins_10_distance_bins.start, layer_1_bin_maxbins_10_distance_bins.stop]"
          }
        ]
      },
      "range": [0, {"signal": "width"}],
      "bins": {"signal": "layer_0_bin_maxbins_10_distance_bins"},
      "zero": false
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "__count"},
          {"data": "data_1", "field": "__count"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "distance (binned)",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/10)"},
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Count of Records",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    }
  ],
  "config": {"axisY": {"minExtent": 30}}
}
