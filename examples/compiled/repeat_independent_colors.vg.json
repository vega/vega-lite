{
  "$schema": "https://vega.github.io/schema/vega/v4.json",
  "autosize": "pad",
  "padding": 5,
  "width": 200,
  "height": 200,
  "data": [
    {"name": "source_0", "url": "data/cars.json", "format": {"type": "json"}},
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "formula",
          "expr": "toNumber(datum[\"Horsepower\"])",
          "as": "Horsepower"
        },
        {
          "type": "formula",
          "expr": "toNumber(datum[\"Miles_per_Gallon\"])",
          "as": "Miles_per_Gallon"
        },
        {
          "type": "filter",
          "expr": "datum[\"Horsepower\"] !== null && !isNaN(datum[\"Horsepower\"]) && datum[\"Miles_per_Gallon\"] !== null && !isNaN(datum[\"Miles_per_Gallon\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "formula",
          "expr": "toNumber(datum[\"Horsepower\"])",
          "as": "Horsepower"
        },
        {
          "type": "formula",
          "expr": "toNumber(datum[\"Miles_per_Gallon\"])",
          "as": "Miles_per_Gallon"
        },
        {
          "type": "filter",
          "expr": "datum[\"Horsepower\"] !== null && !isNaN(datum[\"Horsepower\"]) && datum[\"Miles_per_Gallon\"] !== null && !isNaN(datum[\"Miles_per_Gallon\"])"
        }
      ]
    }
  ],
  "layout": {
    "padding": {"row": 10, "column": 10},
    "columns": 2,
    "bounds": "full",
    "align": "all"
  },
  "marks": [
    {
      "type": "group",
      "name": "child_Origin_group",
      "style": "cell",
      "encode": {
        "update": {"width": {"signal": "width"}, "height": {"signal": "height"}}
      },
      "marks": [
        {
          "name": "child_Origin_marks",
          "type": "symbol",
          "style": ["point"],
          "from": {"data": "data_0"},
          "encode": {
            "update": {
              "opacity": {"value": 0.7},
              "fill": {"value": "transparent"},
              "stroke": {"scale": "child_Origin_color", "field": "Origin"},
              "x": {"scale": "child_Origin_x", "field": "Horsepower"},
              "y": {"scale": "child_Origin_y", "field": "Miles_per_Gallon"}
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "child_Origin_x",
          "orient": "bottom",
          "grid": false,
          "title": "Horsepower",
          "labelFlush": true,
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(width/40)"},
          "zindex": 1
