{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "data/cars.json"},
  "mark": {
    "type": "errorband",
    "extent": "ci",
    "borders": true
  },
  "encoding": {
    "y": {
      "field": "Miles_per_Gallon",
      "type": "quantitative",
      "scale": {"zero": false},
      "title": "Miles per Gallon (95% CIs)"
    },
    "x": {"timeUnit": "year", "field": "Year"}
  }
}
