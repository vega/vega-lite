{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {"url": "data/movies.json"},
  "transform": [
    {
      "bin": true,
      "field": "IMDB_Rating",
      "as": "bin_IMDB_Rating"
    }
  ],
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "bin_IMDB_Rating",
      "title": "IMDB_Rating (binned)",
      "bin": {
        "binned": true,
        "step": 1
      },
      "type": "quantitative"
    },
    "x2": {"field": "bin_IMDB_Rating_end"},
    "y": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}
