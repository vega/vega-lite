{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "layer": [
    {
      "data": {"url": "data/cars.json"},
      "mark": "line",
      "encoding": {
        "x": {"field": "Cylinders","type": "ordinal"},
        "y": {
          "aggregate": "max",
          "bin": false,
          "field": "Horsepower",
          "type": "quantitative"
        },
        "color": {"value": "darkred"}
      }
    },
    {
      "data": {"url": "data/cars.json"},
      "mark": "line",
      "encoding": {
        "x": {"field": "Cylinders","type": "ordinal"},
        "y": {
          "aggregate": "min",
          "bin": false,
          "field": "Horsepower",
          "type": "quantitative"
        }
      }
    }
  ],
  "config": {
    "line": {"point": true}
  }
}
