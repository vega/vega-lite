{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {"url": "data/seattle-weather.csv"},
  "mark": "line",
  "encoding": {
    "x": {
      "timeUnit": "yearmonth",
      "field": "date",
      "type": "temporal"
    },
    "y": {
      "aggregate": "max",
      "field": "temp_max",
      "type": "quantitative"
    }
  }
}