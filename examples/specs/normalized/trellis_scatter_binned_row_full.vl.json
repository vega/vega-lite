{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "A trellis scatterplot showing Horsepower and Miles per gallons, faceted by binned values of Acceleration.",
  "data": {
    "url": "data/cars.json"
  },
  "facet": {
    "row": {
      "field": "Acceleration",
      "type": "quantitative",
      "bin": true
    }
  },
  "spec": {
    "mark": "point",
    "encoding": {
      "x": {
        "field": "Horsepower",
        "type": "quantitative"
      },
      "y": {
        "field": "Miles_per_Gallon",
        "type": "quantitative"
      }
    }
  }
}