{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {"url": "data/seattle-temps.csv"},
  "mark": "point",
  "encoding": {
    "color": {
      "timeUnit": "yearmonth", "field": "date", "type": "temporal"
    },
    "x": {"aggregate": "mean", "field": "temp", "type": "quantitative"}
  }
}
