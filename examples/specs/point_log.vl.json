{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "mark": "point",
  "data": {
    "values": [
      {"x": 0, "y": 1}, {"x": 1, "y": 10},
      {"x": 2, "y": 100}, {"x": 3, "y": 1000},
      {"x": 4, "y": 10000}, {"x": 5, "y": 100000},
      {"x": 6, "y": 1000000}, {"x": 7, "y": 10000000}
    ]
  },
  "encoding": {
    "x": {"field": "x", "type": "quantitative"},
    "y": {
      "field": "y", "type": "quantitative",
      "scale": {"type": "log"}
    }
  }
}
