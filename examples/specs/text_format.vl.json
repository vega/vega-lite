{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {"url": "data/cars.json"},
  "mark": "text",
  "encoding": {
    "y": {
      "field": "Origin",
      "type": "ordinal"
    },
    "text": {
      "aggregate": "mean",
      "field": "Horsepower",
      "type": "quantitative",
      "format": ".2f"
    }
  }
}
