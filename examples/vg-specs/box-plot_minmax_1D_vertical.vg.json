{
    "$schema": "http://vega.github.io/schema/vega/v3.0.json",
    "description": "A vertical 1D box plot showing median, min, and max in the US population distribution of age groups in 2000.",
    "autosize": "pad",
    "padding": 5,
    "encode": {
        "update": {
            "fill": {
                "value": "transparent"
            }
        }
    },
    "data": [
        {
            "name": "source_0",
            "url": "data/population.json",
            "format": {
                "type": "json"
            },
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people\"])",
                    "as": "people"
                },
                {
                    "type": "aggregate",
                    "groupby": [],
                    "ops": [
                        "q1",
                        "q3",
                        "median",
                        "min",
                        "max"
                    ],
                    "fields": [
                        "people",
                        "people",
                        "people",
                        "people",
                        "people"
                    ],
                    "as": [
                        "lowerBox",
                        "upperBox",
                        "midBox",
                        "lowerWhisker",
                        "upperWhisker"
                    ]
                }
            ]
        },
        {
            "name": "data_0",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"lowerWhisker\"])",
                    "as": "lowerWhisker"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"lowerBox\"])",
                    "as": "lowerBox"
                },
                {
                    "type": "aggregate",
                    "groupby": [],
                    "ops": [
                        "q3",
                        "max"
                    ],
                    "fields": [
                        "people",
                        "people"
                    ],
                    "as": [
                        "q3_people",
                        "max_people"
                    ]
                }
            ]
        },
        {
            "name": "data_2",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"lowerBox\"])",
                    "as": "lowerBox"
                },
                {
                    "type": "aggregate",
                    "groupby": [],
                    "ops": [
                        "q1",
                        "q3"
                    ],
                    "fields": [
                        "people",
                        "people"
                    ],
                    "as": [
                        "q1_people",
                        "q3_people"
                    ]
                }
            ]
        },
        {
            "name": "data_3",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"midBox\"])",
                    "as": "midBox"
                },
                {
                    "type": "aggregate",
                    "groupby": [],
                    "ops": [
                        "median"
                    ],
                    "fields": [
                        "people"
                    ],
                    "as": [
                        "median_people"
                    ]
                }
            ]
        }
    ],
    "signals": [
        {
            "name": "width",
            "update": "max(layer_0_width, layer_1_width, layer_2_width, layer_3_width)"
        },
        {
            "name": "height",
            "update": "max(layer_0_height, layer_1_height, layer_2_height, layer_3_height)"
        },
        {
            "name": "layer_0_width",
            "update": "21"
        },
        {
            "name": "layer_0_height",
            "update": "200"
        },
        {
            "name": "layer_1_width",
            "update": "21"
        },
        {
            "name": "layer_1_height",
            "update": "200"
        },
        {
            "name": "layer_2_width",
            "update": "21"
        },
        {
            "name": "layer_2_height",
            "update": "200"
        },
        {
            "name": "layer_3_width",
            "update": "21"
        },
        {
            "name": "layer_3_height",
            "update": "200"
        }
    ],
    "marks": [
        {
            "name": "layer_0_marks",
            "type": "rule",
            "role": "boxWhisker",
            "from": {
                "data": "data_0"
            },
            "encode": {
                "update": {
                    "x": {
                        "value": 10.5
                    },
                    "y": {
                        "scale": "y",
                        "field": "lowerWhisker"
                    },
                    "y2": {
                        "scale": "y",
                        "field": "lowerBox"
                    },
                    "stroke": {
                        "value": "#4c78a8"
                    }
                }
            },
            "clip": true
        },
        {
            "name": "layer_1_marks",
            "type": "rule",
            "role": "boxWhisker",
            "from": {
                "data": "data_1"
            },
            "encode": {
                "update": {
                    "x": {
                        "value": 10.5
                    },
                    "y": {
                        "scale": "y",
                        "field": "upperBox"
                    },
                    "y2": {
                        "scale": "y",
                        "field": "upperWhisker"
                    },
                    "stroke": {
                        "value": "#4c78a8"
                    }
                }
            },
            "clip": true
        },
        {
            "name": "layer_2_marks",
            "type": "rect",
            "role": "box",
            "from": {
                "data": "data_2"
            },
            "encode": {
                "update": {
                    "xc": {
                        "value": 10.5
                    },
                    "width": {
                        "value": 14
                    },
                    "y": {
                        "scale": "y",
                        "field": "lowerBox"
                    },
                    "y2": {
                        "scale": "y",
                        "field": "upperBox"
                    },
                    "fill": {
                        "value": "#4c78a8"
                    }
                }
            },
            "clip": true
        },
        {
            "name": "layer_3_marks",
            "type": "rect",
            "role": "boxMid",
            "from": {
                "data": "data_3"
            },
            "encode": {
                "update": {
                    "xc": {
                        "value": 10.5
                    },
                    "yc": {
                        "scale": "y",
                        "field": "midBox"
                    },
                    "width": {
                        "value": 14
                    },
                    "height": {
                        "value": 1
                    },
                    "fill": {
                        "value": "white"
                    },
                    "opacity": {
                        "value": 0.7
                    }
                }
            },
            "clip": true
        }
    ],
    "scales": [
        {
            "name": "y",
            "type": "linear",
            "domain": {
                "fields": [
                    {
                        "data": "data_0",
                        "field": "lowerWhisker"
                    },
                    {
                        "data": "data_0",
                        "field": "lowerBox"
                    },
                    {
                        "data": "data_1",
                        "field": "upperBox"
                    },
                    {
                        "data": "data_1",
                        "field": "upperWhisker"
                    },
                    {
                        "data": "data_2",
                        "field": "lowerBox"
                    },
                    {
                        "data": "data_2",
                        "field": "upperBox"
                    },
                    {
                        "data": "data_3",
                        "field": "midBox"
                    }
                ],
                "sort": true
            },
            "range": [
                200,
                0
            ],
            "round": true,
            "nice": true,
            "zero": true
        }
    ],
    "axes": [
        {
            "title": "population",
            "scale": "y",
            "orient": "left",
            "zindex": 1
        },
        {
            "scale": "y",
            "domain": false,
            "grid": true,
            "labels": false,
            "orient": "left",
            "ticks": false,
            "zindex": 0
        }
    ]
}
