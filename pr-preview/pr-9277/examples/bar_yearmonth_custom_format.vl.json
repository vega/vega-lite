{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Custom time format on x-axis.",
  "data": {"url": "data/seattle-weather.csv"},
  "mark": "bar",
  "encoding": {
    "x": {
      "timeUnit": "yearmonth", "field": "date",
      "axis": {"format": "%B of %Y"}
    },
    "y": {"aggregate": "mean", "field": "temp_max"}
  }
}
