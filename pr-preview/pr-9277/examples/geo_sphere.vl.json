{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 200,
  "height": 200,
  "projection": {
    "type": "orthographic",
    "scale": 100,
    "translate": [
      100,
      100
    ]
  },
  "layer": [
    {
      "data": {
        "sphere": true
      },
      "mark": {
        "type": "geoshape",
        "fill": "aliceblue"
      }
    },
    {
      "data": {
        "graticule": true
      },
      "mark": {
        "type": "geoshape",
        "stroke": "black",
        "strokeWidth": 0.5
      }
    }
  ]
}
